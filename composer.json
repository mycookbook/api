{
    "name": "cookbook/api",
    "description": "The official cookbookshq api",
    "keywords": [
        "php",
        "cookbookshq",
        "api"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.5|^8.0",
        "barryvdh/laravel-cors": "^3.0",
        "tymon/jwt-auth": "dev-develop",
        "league/fractal": "1.0.x-dev",
        "guzzlehttp/guzzle": "7.4.5",
        "symfony/console": "^6.0.3",
        "doctrine/dbal": "^2.10",
        "symfony/http-kernel": "^6.0",
        "ext-exif": "*",
        "laravel/tinker": "^2.5",
        "ext-json": "*",
        "pusher/pusher-php-server": "^4.1",
        "league/flysystem": "^3.0.16",
        "laravel/framework": "^9.0",
        "fruitcake/laravel-cors": "^2.0",
        "fideloper/proxy": "^4.4",
        "spatie/ignition": "^1.0",
        "php-open-source-saver/jwt-auth": "^1.4",
        "nunomaduro/collision": "6.2.1",
        "socialiteproviders/tiktok": "^4.1",
        "socialiteproviders/twitter": "^4.1",
        "socialiteproviders/pinterest": "^4.2",
        "socialiteproviders/instagram": "^4.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "ext-json": "*",
        "fakerphp/faker": "1.19.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "tests/",
            "database/seeds"
        ],
        "files": [
            "app/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
