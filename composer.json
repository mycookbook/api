{
    "name": "cookbook/api",
    "description": "The official cookbookshq api",
    "keywords": ["php", "cookbookshq", "api"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "laravel/lumen-framework": "6.x-dev",
        "vlucas/phpdotenv": "3.3.x-dev",
        "fzaninotto/faker": "1.9.1",
        "barryvdh/laravel-cors": "^1.0",
        "tymon/jwt-auth": "1.0.0",
        "league/fractal": "1.0.x-dev",
        "appzcoder/lumen-routes-list": "^1.1",
        "guzzlehttp/guzzle": "6.5.2",
        "php-coveralls/php-coveralls": "2.2.0",
        "symfony/console": "4.4.7",
        "doctrine/dbal": "^2.10",
        "sentry/sentry-laravel": "1.7.1",
        "illuminate/mail": "6.x-dev",
        "symfony/http-kernel": "4.4.13",
      "ext-exif": "*",
        "laravel/tinker": "^2.4",
      "ext-json": "*",
        "pusher/pusher-php-server": "^4.1",
        "illuminate/routing": "6.x-dev",
        "league/flysystem": " ~1.0",
      "ext-pdo": "*"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": ">=5.0",
      "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
      "classmap": [
        "tests/",
        "database/seeds"
      ],
      "files": [
        "app/Http/helpers.php"
      ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
