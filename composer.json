{
  "name": "cookbook/api",
  "description": "The official cookbookshq api",
  "keywords": ["php", "cookbookshq", "api"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1",
    "laravel/lumen-framework": "^7.0",
    "vlucas/phpdotenv": "^4.0",
    "fzaninotto/faker": "1.9.1",
    "barryvdh/laravel-cors": "^1.0",
    "tymon/jwt-auth": "1.0.0",
    "league/fractal": "1.0.x-dev",
    "guzzlehttp/guzzle": "6.5.8",
    "php-coveralls/php-coveralls": "2.2.0",
    "symfony/console": "^5.0",
    "doctrine/dbal": "^2.10",
    "illuminate/mail": "^7.0",
    "symfony/http-kernel": "^5.0",
    "ext-exif": "*",
    "laravel/tinker": "^2.4",
    "ext-json": "*",
    "pusher/pusher-php-server": "^4.1",
    "illuminate/routing": "^7.0",
    "league/flysystem": " ~1.0",
    "sentry/sentry-laravel": "^1.9"
  },
  "require-dev": {
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": ">=5.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "tests/",
      "database/seeds"
    ],
    "files": [
      "app/Http/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/",
      "database/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
